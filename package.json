{
	"name": "vrc-auton",
	"displayName": "VRC Auton Builder",
	"description": "Autonomous builder for the Vex Robotics Competition",
	"version": "0.0.2",
	"publisher": "AndrewCurtis",
	"license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Microsoft/vscode-extension-samples"
  },
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/server/server/extension.js",
  "contributes": {
    "customEditors": [
      {
        "viewType": "catCustoms.pawDraw",
        "displayName": "Paw Draw",
        "selector": [
          {
            "filenamePattern": "*.vauto"
          }
        ]
      }
    ],
    "commands": [
      {
        "command": "catCustoms.pawDraw.new",
        "title": "Create new Paw Draw Document",
        "category": "Paw Draw"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc --project ./src/webview/tsconfig.json && tsc --project ./src/server/tsconfig.json",
    "lint": "eslint \"src/**/*.ts\"",
    "watch": "tsc -w -p ./"
  },
  "devDependencies": {
    "@types/node": "^16.11.7",
    "@types/vscode": "^1.73.0",
    "@typescript-eslint/eslint-plugin": "^5.42.0",
    "@typescript-eslint/parser": "^5.42.0",
    "eslint": "^8.26.0",
    "typescript": "^4.8.4"
  }
}
